<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.wxiaoqi.security.hrsystem.mapper.WebFuncFrameMapper">
  <resultMap id="WebFuncFrameResultMap" type="com.github.wxiaoqi.security.hrsystem.entity.WebFuncFrame">
    <result column="Func_Code" property="funccode"/>
    <result column="ModuleID" property="moduleid"/>
    <result column="Func_Name" property="funcname"/>
    <result column="Func_Url" property="funcurl"/>
    <result column="Func_Parent" property="funcparent"/>
    <result column="FunTag" property="funtag"/>
    <result column="fa" property="fa"/>
  </resultMap>
  
  <select id="selectByfunccode" resultMap="WebFuncFrameResultMap">
    select
    Func_Code "funccode",
    ModuleID "moduleid",
    Func_Name "funcname",
    Func_Url "funcurl",
    Func_Parent "funcparent",
    FunTag "funtag",
    fa "fa"
    from WebFuncFrame where Func_Code IN
    <foreach item="item" index="index" collection="funccode" open="(" separator="," close=")">
      #{item}
    </foreach>

  </select>

  <select id="selectByfunccodeAndmodid" resultMap="WebFuncFrameResultMap">
    select
    Func_Code "funccode",
    ModuleID "moduleid",
    Func_Name "funcname",
    Func_Url "funcurl",
    Func_Parent "funcparent",
    FunTag "funtag",
    fa "fa"
    from WebFuncFrame where Func_Code IN
    <foreach item="item" index="index" collection="funccode" open="(" separator="," close=")">
      #{item}
    </foreach>
    and ModuleID = #{modeid}
  </select>


  <select id="callWebGetFuncView" resultMap="WebFuncFrameResultMap">
     exec WebGetFuncView
          '',
          #{mid},
          #{A0188}

  </select>

</mapper>