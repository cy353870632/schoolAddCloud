<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.wxiaoqi.security.xjsystem.mapper.ProvinceMapper">

    <resultMap id="ProvinceResultMap" type="com.github.wxiaoqi.security.xjsystem.entity.Province">
    </resultMap>
    <resultMap id="ProvinceVoResultMap" type="com.github.wxiaoqi.security.xjsystem.vo.ProvinceVo">
    </resultMap>

    <select id="selectAllByStatus" resultMap="ProvinceVoResultMap">
        select * from province WHERE status=#{status}
    </select>
</mapper>