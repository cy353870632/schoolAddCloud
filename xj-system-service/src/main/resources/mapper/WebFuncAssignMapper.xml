<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.wxiaoqi.security.hrsystem.mapper.WebFuncAssignMapper">
  <resultMap id="WebFuncAssignResultMap" type="com.github.wxiaoqi.security.hrsystem.entity.WebFuncAssign">
  </resultMap>
  
  <select id="selectFunccodeByActorid" resultType="java.lang.String">
        select Func_Code "funccode" from WebFunAssign where ActorId IN
        <foreach item="item" index="index" collection="actoridlist" open="(" separator="," close=")">
          #{item}
        </foreach>
  </select>

</mapper>